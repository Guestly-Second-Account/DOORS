--// Load Spawner
local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/Voor-Pr00/Fogroom/refs/heads/main/Spawner%20v3"))()
if not spawner or not spawner.Create then
    warn("Spawner không tải được hoặc thiếu hàm Create!")
    return
end

--// Tạo entity
local entity = spawner.Create({
	Entity = {
		Name = "Darkness",
		Asset = "rbxassetid://121725853188313",
		HeightOffset = 0
	},
	Lights = {
		Flicker = {
			Enabled = false,
			Duration = 4
		},
		Shatter = true,
		Repair = false
	},
	Light = {
		Color = {1, 1, 1}
	},
	Earthquake = { Enabled = false },
	CameraShake = {
		Enabled = false,
		Range = 100,
		Values = {2.5, 35, 0.15, 2}
	},
	Movement = {
		Speed = 15,
		Delay = 0,
		Reversed = false
	},
	Rebounding = {
		Enabled = false,
		Type = "Ambush",
		Min = 1,
		Max = 1,
		Delay = 2
	},
	Damage = {
		Enabled = true,
		Range = 30,
		Amount = 100
	},
	Crucifixion = {
		Enabled = true,
		Range = 40,
		Resist = false,
		Break = true
	},
	Death = {
		Type = "Guiding",
		Hints = {
			"Welcome back, what did you get now?",
			"Hm.. I don't really know who you dead to",
			"Maybe you can call him Darkness",
			"Whenever he appears, you will hear the wind noises",
			"Anyways you can do it! Bye!! :>"
		},
		Cause = ""
	}
})

--// Hàm hỗ trợ tải âm thanh từ link GitHub
local function GitAud(soundgit, filename)
    local url = soundgit
    local FileName = filename
    writefile(FileName .. ".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName .. ".mp3")
end

local function CustomGitSound(soundlink, vol, filename)
    local sound = Instance.new("Sound")
    sound.SoundId = GitAud(soundlink, filename)
    sound.Parent = workspace
    sound.Volume = vol or 1
    sound.PlaybackSpeed = 1
    sound.Name = "dam"
    sound:Play()
end

--// Hàm Jumpscare
local function DoJumpscare()
	local player = game.Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")

	local screenGui = Instance.new("ScreenGui")
	screenGui.Name = "Jumpscare"
	screenGui.Parent = playerGui
	screenGui.IgnoreGuiInset = true

	local red = Instance.new("ImageLabel")
	red.Name = "Jumpscare"
	red.Size = UDim2.new(1, 0, 1, 0)
	red.BackgroundTransparency = 1
	red.Image = "rbxassetid://89889789364498"
	red.ImageTransparency = 0
	red.Parent = screenGui

	CustomGitSound("https://github.com/Guestly-Second-Account/DOORS/raw/refs/heads/main/XRecorder_Edited_20250903_01.mp3?raw=true", 1, "dam")

	task.wait(2)
	local TweenService = game:GetService("TweenService")
	local TW = TweenService:Create(red, TweenInfo.new(3), {ImageTransparency = 1})
	TW:Play()
	task.wait(1)

	if workspace:FindFirstChild("dam") then
		workspace.dam:Destroy()
	end
	screenGui:Destroy()
end

--// Callbacks
entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
    local foot = workspace:FindFirstChild("Darkness") and workspace.Darkness:FindFirstChild("RushNew")
    if foot and foot:FindFirstChild("Footsteps") then
        foot.Footsteps.Volume = 0.1
    end
end)

entity:SetCallback("OnStartMoving", function()
    print("Entity has started moving")
end)

entity:SetCallback("OnEnterRoom", function(room, firstTime)
    if firstTime then
        print("Entity has entered room: " .. room.Name .. " for the first time")
    else
        print("Entity has entered room: " .. room.Name .. " again")
    end
end)

entity:SetCallback("OnLookAt", function(lineOfSight)
	if lineOfSight then
		print("Player is looking at entity")
		if workspace:FindFirstChild("Darkness") then
			workspace.Darkness:Destroy()
		end
	else
		print("Player view is obstructed")
	end
end)

entity:SetCallback("OnRebounding", function(startOfRebound)
    if startOfRebound then
        print("Entity has started rebounding")
	else
        print("Entity has finished rebounding")
	end
end)

entity:SetCallback("OnDespawning", function()
    print("Entity is despawning")
end)

entity:SetCallback("OnDespawned", function()
    print("Entity has despawned")
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
		if workspace:FindFirstChild("Darkness") then
			workspace.Darkness:Destroy()
		end
		DoJumpscare()
	else
		print("Entity has damaged the player")
		if workspace:FindFirstChild("Darkness") then
			workspace.Darkness:Destroy()
		end
		DoJumpscare()
	end
end)

--// Run entity
entity:Run()

local AntiDoorOpener = game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
game.Players.LocalPlayer.Character.Humanoid:TakeDamage(1000)
game:GetService("ReplicatedStorage").GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Failed to load data"
end)

-- Loading Ui
loadstring(game:HttpGet("https://raw.githubusercontent.com/DoorsHubs/Tutorial/refs/heads/main/Loading%20Ui"))()
wait(1)
local caption = game.Players.LocalPlayer:WaitForChild("PlayerGui").MainUI.MainFrame.Caption
caption.TextColor3 = Color3.fromRGB(3, 255, 11)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Degenerate Mode Actived!",true)
wait(2)
local caption = game.Players.LocalPlayer:WaitForChild("PlayerGui").MainUI.MainFrame.Caption
caption.TextColor3 = Color3.fromRGB(3, 255, 11)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Credit by Guestly",true)

-- Doors sound changer
coroutine.wrap(function()
    while true do
        wait()
        workspace.CurrentRooms[game.ReplicatedStorage.GameData.LatestRoom.Value].Door.Door.Unlock.SoundId = "rbxassetid://0"

workspace.CurrentRooms[game.ReplicatedStorage.GameData.LatestRoom.Value].Door.Door.Open.SoundId = "rbxassetid://6431334087"

workspace.CurrentRooms[game.ReplicatedStorage.GameData.LatestRoom.Value].Door.Door.Bell.SoundId = "rbxassetid://0"
        workspace.CurrentRooms[game.ReplicatedStorage.GameData.LatestRoom.Value].Door.Door.SlamOpen.SoundId = "rbxassetid://6431334087"
        workspace.CurrentRooms[game.ReplicatedStorage.GameData.LatestRoom.Value].Door.Door.SlamOpen.TimePosition = 0
    end
end)()

-- Jump Enable
game.Players.LocalPlayer.Character:SetAttribute("CanJump", true)

-- Sprint Bar
loadstring(game:HttpGet("https://raw.githubusercontent.com/DoorsHubs/Doors-Hubs/refs/heads/main/Hardcore%20Sprint%20System%20(PC%20and%20Mobile%20Adaptation).lua.txt"))()

-- Toxic Bar
local tweenservice = game:GetService("TweenService")

local gui = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)
local frame = Instance.new("Frame",gui)
frame.BorderSizePixel = 0
frame.BackgroundTransparency = 1
frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)  
frame.Position = UDim2.new(1, -320, 1, -35)
frame.Size = UDim2.new(0, 310,0, 25)
local corner = Instance.new("UICorner",frame)
corner.CornerRadius = UDim.new(0.25,0)
local stroke = Instance.new("UIStroke",frame)
stroke.Color = Color3.fromRGB(255, 224, 189) 
stroke.Thickness = 4

local bar = Instance.new("Frame",frame)
bar.Position = UDim2.new(0.016, 0,0.19, 0)
bar.Size = UDim2.new(0, 50,0, 15)
bar.BackgroundColor3 = Color3.fromRGB(8,255,8) 
bar.BorderSizePixel = 0
local corner1 = corner:Clone()
corner1.Parent = bar

local num = Instance.new("NumberValue",frame)
num.Value = 0

local scriptURLs = {
    "https://raw.githubusercontent.com/DoorsHubs/Doors-Hubs/refs/heads/main/Death%20v2"
}

num.Changed:Connect(function()
    local anim = tweenservice:Create(bar,TweenInfo.new(5,Enum.EasingStyle.Linear),{Size = UDim2.new(0,num.Value,0,15)})
    anim:Play()
if num.Value >= 300 then
wait(15)
num.Value = 0 
local index = math.random(1, #scriptURLs)
        local url = scriptURLs[index]
        local success, result = pcall(function()
            return game:HttpGet(url)
        end)
        if success then
            local func, err = loadstring(result)
            if func then
                func()
end
end
end
end)

_G.stats = num
_G.baseval = -2
task.wait(0)
_G.stats.Value += 1

game:GetService("TextChatService").TextChannels.RBXGeneral:DisplaySystemMessage("<font color='#26ff00'>[Degenerate Mode]: Script Succesfully Executed/Loaded.</font>")

AntiDoorOpener:Disconnect()

-- Intro
loadstring(game:HttpGet("https://raw.githubusercontent.com/DoorsHubs/Tutorial/refs/heads/main/Intro"))()

